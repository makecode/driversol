            a.breadcrumbs__link(href='#') Video cards AMD
          li.breadcrumbs__el
            a.breadcrumbs__link(href='#') AMD Mobility FireGL
          li.breadcrumbs__el
            a.breadcrumbs__link(href='#') Mobility FireGL

    section.section.is-3-inner
      div.container
        div.top-head
          h1.section__title AMD Mobility FireGL video cards drivers
          a(href='#').top-head__logo
            img(src=require('../../../media/images/amd_logo.png'))

        div.btn-automatically
          a(href='#').btn.is-green.is-big
            i.icon.icon-download
            span Install drivers automatically

        div.drivers-head
          span.drivers-head__title Available drivers (3)

          div.select
            select#slct
              option All systems
              option Window 10 x86
              option Window 10 x86
              option Window 10 x86
              option Window 10 x86
              option Window 10 x86

        div.driver
          div.driver__top
            div.driver__wrap
              div.driver__left
                h2 AMD Mobility FireGL
                div.driver__list
                  dl
                    dt Driver type:
                    dd
                      a(href='#') Display adapters
                  dl
                    dt Version:
                    dd 14.200.0.0081 (16 Jun 2014)
                  dl
                    dt Vendor:
                    dd
                      a(href='#') Advanced Micro Devices Inc
                  dl
                    dt *.inf file:
                    dd c8150272.inf

              div.driver__right
                div.download-block
                  div.btn-install
                    a(href='#').btn.is-green.full-width
                      i.icon.icon-download
                      span Install driver
                  div.btn-download
                    a(href='#').btn.transparent
                      i.icon.icon-download-blue
                      span Download Driver
                      span.grey  (20.53 MB)
                  div.version
                    i.icon.icon-win
                    span.version__name Windows Vista x64, 7 x64, 8 x64, 8.1 x64, 10 x64

          div.driver__bottom
            div.hardware
              nobr.toggle
                i.icon.icon-settings
                span.hardware__name Hardware IDs (12)
